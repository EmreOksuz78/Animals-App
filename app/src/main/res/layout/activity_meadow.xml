<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".meadow.Meadow">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/meadow"
        android:scaleType="fitXY"
        />

    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="153dp"
        android:layout_height="153dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="240dp"
        android:layout_marginBottom="335dp"
        android:onClick="horse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/at" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="153dp"
        android:layout_height="153dp"
        android:layout_marginStart="240dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="335dp"
        android:onClick="rabbit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/tavsan" />

    <ImageView
        android:id="@+id/imageView47"
        android:layout_width="153dp"
        android:layout_height="153dp"
        android:layout_marginBottom="130dp"
        android:onClick="sheep"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/koyun" />
</androidx.constraintlayout.widget.ConstraintLayout>